# Production Environment Configuration
# ====================================

# API Configuration
VITE_API_URL=https://your-production-api.vercel.app
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=production
VITE_BUILD_VERSION=1.0.0

# OneSignal Push Notifications
VITE_ONESIGNAL_APP_ID=your_production_onesignal_app_id
VITE_ONESIGNAL_SAFARI_WEB_ID=your_production_safari_web_id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_PWA=true
VITE_ENABLE_ERROR_REPORTING=true

# Performance Settings
VITE_POLLING_INTERVAL=10000
VITE_CACHE_DURATION=300000
VITE_MAX_UPLOAD_SIZE=10485760

# Security Settings
VITE_ENABLE_CSP=true
VITE_SECURE_COOKIES=true

# Monitoring
VITE_SENTRY_DSN=your_production_sentry_dsn
VITE_SENTRY_ENVIRONMENT=production
VITE_ENABLE_PERFORMANCE_MONITORING=true

# CDN Configuration
VITE_CDN_URL=https://your-cdn.cloudfront.net
VITE_STATIC_ASSETS_URL=https://your-assets.s3.amazonaws.com