# Staging Environment Configuration
# =================================

# API Configuration
VITE_API_URL=https://your-staging-api.vercel.app
VITE_API_TIMEOUT=30000

# Environment
VITE_NODE_ENV=staging
VITE_BUILD_VERSION=1.0.0-staging

# OneSignal Push Notifications
VITE_ONESIGNAL_APP_ID=your_staging_onesignal_app_id
VITE_ONESIGNAL_SAFARI_WEB_ID=your_staging_safari_web_id

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_PWA=true
VITE_ENABLE_ERROR_REPORTING=true
VITE_ENABLE_DEBUG_MODE=true

# Performance Settings
VITE_POLLING_INTERVAL=5000
VITE_CACHE_DURATION=60000
VITE_MAX_UPLOAD_SIZE=10485760

# Security Settings
VITE_ENABLE_CSP=false
VITE_SECURE_COOKIES=false

# Monitoring
VITE_SENTRY_DSN=your_staging_sentry_dsn
VITE_SENTRY_ENVIRONMENT=staging
VITE_ENABLE_PERFORMANCE_MONITORING=true

# Testing
VITE_ENABLE_TEST_DATA=true
VITE_MOCK_API_DELAY=1000