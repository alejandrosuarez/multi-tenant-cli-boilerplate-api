# Database Configuration
SUPABASE_URL=your_supabase_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Authentication
CLERK_JWKS_URL=your_clerk_jwks_url
CLERK_JWT_AUDIENCE=your_clerk_jwt_audience
CLERK_JWT_ISSUER=your_clerk_jwt_issuer
JWT_SECRET=your_jwt_secret

# API Security
API_TOKENS=your_api_token_1,your_api_token_2,your_api_token_3
ALLOWED_DOMAINS=http://localhost:3000,https://yourdomain.com,https://anotherdomain.com

# Notification Services
ONESIGNAL_API_KEY=your_onesignal_api_key
ONESIGNAL_APP_ID=your_onesignal_app_id

# Email Services
RESEND_API_KEY=your_resend_api_key
RESEND_FROM_EMAIL=noreply@yourdomain.com

# Error Tracking
SENTRY_DSN_BACKEND=your_sentry_dsn
SENTRY_ENV=development

# Application
NODE_ENV=development
